
{
  "version": 2,
  "name": "micro-sitemap-to-array",
  "scope": "telemark-dev",
  "alias": [
    "sitemap.tjeneste.win"
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "builds": [
    { "src": "/index.js", "use": "@now/node" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/index.js" }
  ]
}